<Window x:Class="ProjekatHCI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjekatHCI"
        xmlns:prop="clr-namespace:ProjekatHCI.Util"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="137*"/>
            <ColumnDefinition Width="259*"/>
        </Grid.ColumnDefinitions>
        <Label Name="labela" Content="{prop:LocalizationExtension msg}" Grid.ColumnSpan="2" />
        <Button Name="btn" Click="ChangeLang" Margin="193,108,378,252" Content="{prop:LocalizationExtension logInBtn}" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
